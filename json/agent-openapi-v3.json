                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "title": "Response Get Agent Last Active Agent Last Active Get",
                                    "type": "string",
                                    "format": "date-time"
