                        "type": "integer"
                    },
                    "action_space": {
                        "title": "Action Space",
                        "type": "integer"
                    },
                    "encoded_config": {
                        "title": "Encoded Config",
                        "type": "string"
                    },
                    "encoded_network": {
                        "title": "Encoded Network",
                        "type": "string"
                    },
                    "encoded_buffer": {
                        "title": "Encoded Buffer",
