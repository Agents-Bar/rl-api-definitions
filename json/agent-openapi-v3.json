                    "model": {
                        "title": "Model",
                        "type": "string"
                    },
                    "hyperparameters": {
                        "title": "Hyperparameters",
                        "type": "object"
                    },
                    "last_active": {
                        "title": "Last Active",
                        "type": "string",
                        "format": "date-time"
                    }
                }
