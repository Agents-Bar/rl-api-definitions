                        "schema": {
                            "title": "Model Type",
                            "type": "string"
                        },
                        "name": "model_type",
                        "in": "query"
                    },
                    {
                        "required": true,
                        "schema": {
                            "title": "Obs Size",
                            "type": "integer"
                        },
                        "name": "obs_size",
                        "in": "query"
                    },
                    {
                        "required": true,
                        "schema": {
                            "title": "Action Size",
                            "type": "integer"
                        },
                        "name": "action_size",
                        "in": "query"
                    },
                    {
                        "required": false,
                        "schema": {
                            "title": "Network State",
                            "type": "string",
                            "format": "binary"
                        },
                        "name": "network_state",
                        "in": "query"
                    },
                    {
                        "required": false,
                        "schema": {
                            "title": "Buffer State",
                            "type": "string",
                            "format": "binary"
                        },
                        "name": "buffer_state",
                        "in": "query"
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "title": "Model Config",
                                "type": "object",
                                "additionalProperties": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/agent/{agent_name}": {
            "delete": {
                "summary": "Delete Agent",
                "description": "Assumption is that the service contains only one agent.",
                "operationId": "delete_agent_agent__agent_name__delete",
                "parameters": [
                    {
                        "required": true,
                        "schema": {
                            "title": "Agent Name",
                            "type": "string"
                        },
                        "name": "agent_name",
                        "in": "path"
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Successful Response"
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
