    "components": {
        "schemas": {
            "AgentAction": {
                "title": "AgentAction",
                "required": [
                    "action"
                ],
                "type": "object",
                "properties": {
                    "action": {
                        "title": "Action",
                        "anyOf": [
                            {
                                "type": "integer"
                            },
                            {
                                "type": "number"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "integer"
                                }
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "number"
                                }
                            }
                        ]
                    }
                }
